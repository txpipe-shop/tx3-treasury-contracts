{
  "name": "@sundaeswap/treasury-funds",
  "version": "0.0.2",
  "type": "module",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "LICENSE.md"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts,.tsx' --out-dir './dist/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts,.tsx' --out-dir 'dist/cjs' --source-maps",
    "build:cli": "bun build ./cli/cli.ts --outdir=./dist/cli --target=node",
    "types": "tsc --project ./tsconfig.build.json",
    "build": "bun clean && bun types && bun build:esm && bun build:cjs && bun build:cli && bun set-cjs",
    "watch": "bun clean && bun run build && bun build:esm --watch",
    "set-cjs": "echo '{ \"type\": \"commonjs\" }' > ./dist/cjs/package.json",
    "version": "standard-version",
    "lint": "eslint src --ignore-pattern '**/dist/**'",
    "lint:fix": "bun lint --fix",
    "prepare": "cd .. && husky ./offchain/.husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bun lint:fix --quiet"
    ]
  },
  "bin": "./dist/cli/cli.js",
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@blaze-cardano/emulator": "link:@blaze-cardano/emulator",
    "@blaze-cardano/blueprint": "^0.6.0",
    "@sundaeswap/babel-preset": "^2.0.15",
    "@sundaeswap/eslint-config": "^2.0.15",
    "@types/bun": "latest",
    "@types/commander": "^2.12.5",
    "commander": "^14.0.0",
    "eslint": "^9.27.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "standard-version": "^9.5.0"
  },
  "dependencies": {
    "@blaze-cardano/data": "^0.6.0",
    "@blaze-cardano/query": "link:@blaze-cardano/query",
    "@blaze-cardano/sdk": "link:@blaze-cardano/sdk",
    "@blaze-cardano/uplc": "^0.3.1",
    "@inquirer/prompts": "^7.5.3",
    "cbor": "^10.0.3",
    "clipboardy": "^4.0.0"
  }
}
